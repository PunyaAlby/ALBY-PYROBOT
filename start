_starter () {
    echo "Downloading source code"
    git clone https://github.com/PunyaAlby/ALBY-PYROBOT
    cd ALBY-PYROBOT/ && rm -rf .git
    python3 ../updater.py ../requirements.txt requirements.txt
    echo "ALBY-PYROBOT v0.2.0, Copyright Â© 2022 (ALBY) <https://github.com/PunyaAlby>"
    python3 -m PunyaAlby
}

_starter
